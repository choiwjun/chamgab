# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
ADMIN_BOOTSTRAP=false
ADMIN_EMAILS=admin1@example.com,admin2@example.com

# Kakao Maps
NEXT_PUBLIC_KAKAO_MAP_KEY=your-kakao-map-key

# Naver OAuth (네이버 로그인)
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret

# ML API
ML_API_URL=http://localhost:8000
NEXT_PUBLIC_ML_API_URL=http://localhost:8002
ML_ADMIN_TOKEN=change-me

# Anonymous (guest) analysis daily limit (per IP hash)
ANON_DAILY_ANALYSIS_LIMIT=3

# Credit costs per analysis product
CREDIT_COST_COMMERCIAL=1
CREDIT_COST_HOME_PRICE=2
CREDIT_COST_LAND=4

# Upstash Redis
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token

# Sentry (Optional)
NEXT_PUBLIC_SENTRY_DSN=

# Environment
NODE_ENV=development
